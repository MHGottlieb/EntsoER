Package: EntsoER
Title: Helps Pull Data from Large ENTSO-E CSV-Files
Version: 0.0.0.9000
Authors@R: 
    person("Magnus Gottlieb", email = "magnus.gottlieb@gmail.com", role = c("aut", "cre"))
